{
	"editor.fontFamily": "'Cascadia Code', 'Jetbrains Mono', 'Inconsolata', 'Inconsolata NF', 'Fira Code', Consolas, 'Courier New', monospace",
	"editor.fontLigatures": true,
	"editor.fontSize": 15,
	"editor.semanticHighlighting.enabled": true,
	"editor.experimental.pasteActions.enabled": true,
	"editor.stickyScroll.enabled": true,
	"editor.formatOnSave": true,
	"editor.fastScrollSensitivity": 5,
	"rust-analyzer.rustfmt.rangeFormatting.enable": true,
	"editor.bracketPairColorization.enabled": true,
	"editor.codeLens": true,
	"editor.codeLensFontFamily": "'Jetbrains Mono', Consolas, 'Courier New', monospace",
	"workbench.colorTheme": "GitHub Dark",
	"terminal.integrated.fontFamily": "Hasklug NF",
	"terminal.integrated.env.linux": {
		"ETHERCAT_PATH": "/home/pan/ethercat_ws/igh",
	},
	"editor.semanticTokenColorCustomizations": {
		"rules": {
			"*.mutable": {
				"foreground": "#00ff8f",
				"fontStyle": "underline"
			},
			"*.async": {
				"fontStyle": "italic"
			},
			"*.clojure": {
				"fontStyle": "italic underline"
			},
			"*.attribute": {
				"italic": false,
				"fontStyle": "bold"
			},
			"*.function": {
				"fontStyle": "bold"
			},
		},
		"enabled": true
	},
	"cSpell.words": [
		"minigrep",
		"mwebsrv",
		"repr"
	]
}